<settings>
	<chart id="svgchart" class="chart" width="650" height="400" type="scatter" style="shape-rendering: crispEdges; text-rendering: optimizeLegibility;" />
	
	<title id="title" class="title" x="0" y="0" width="650" height="50" style="text-anchor: middle;">
		<title id="title-text" class="title">The title</title>
		<subtitle id="subtitle-text" class="subtitle" /> <!-- The text is blank in the default so I can check for None to determine whether or not to include a title -->
	</title>
	
	<x-axis id="x-axis" class="axis" style="stroke: black; stroke-width: 1px;">
		<majors count="5" size="10" id-prefix="x-axis-major" class="major tick" />
		<minors count="25" size="5" id-prefix="x-axis-minor" class="minor tick" />
		<base id="x-axis-base" class="axis-base" />
		<labels id-prefix="x-axis-label" class="axis-label" format="%d" style="text-anchor: middle; stroke: none;" />
		<grid id="x-grid" class="grid">
			<lines id-prefix="x-grid-line" class="grid-line" style="stroke: gray; stroke-width: 1px;" />
		</grid>
		<range min="auto" max="auto" />
	</x-axis>
	
	<y-axis id="y-axis" class="axis" baseid="y-axis-base" baseclass="axis-base" style="stroke: black; stroke-width: 1px;">
		<majors count="5" size="10" id-prefix="y-axis-major" class="major tick" />
		<minors count="25" size="5" id-prefix="y-axis-minor" class="minor tick" />
		<base id="y-axis-base" class="axis-base" />
		<labels id-prefix="y-axis-label" class="y-axis-label" format="%d" style="text-anchor: end; stroke: none;" />
		<grid id="y-grid" class="grid">
			<lines id-prefix="y-grid-line" class="grid-line" />
		</grid>
		<range min="auto" max="auto" />
	</y-axis>
	
	<datafield x="150" y="50" width="480" height="300" id="data-field" class="data-field" />
	
	<datasets id="data-sets" class="data-sets">
		<set id="set1" class="set1 dataset" label="Set 1">
			<symbol shape="circle" size="5" id-prefix="set1-datapoint" class="set1-datapoint" style="fill: green" />
			<line id="set1-line" class="set1-dataline" style="stroke: black; stroke-width: 1px;" />
			<legend-entry id="legend-entry-set1" class="legend-entry" >
				<symbol id="legend-shape-set1" class="legend-shape" style="fill: green" />
				<label id="legend-label-set1" class="legend-label" />
			</legend-entry>
		</set>
	</datasets>
	
	<legend id="legend" class="legend" x="5" y="15" width="100">
		<title size="12" id="legend-title" class="legend-title" style="text-anchor: middle; stroke: none;">Legend</title>
		<entries height="12" />
	</legend>
</settings>